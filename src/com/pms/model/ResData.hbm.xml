<?xml version="1.0" encoding="UTF-8"?>
<!-- 指定Hiberante3映射文件的DTD信息 -->
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.pms.model">
	<!-- 每个class元素对应一个持久化对象 -->
	<class name="ResData" table="WA_AUTHORITY_DATA_RESOURCE">
		<!-- id元素定义持久化类的标识属性 -->
		<id name="id" type="java.lang.Integer">
			<generator class="identity"/>
		</id>
		<!-- property元素定义常规属性 -->
		<property name="name" type="java.lang.String" length="64"/>
		<property name="resource_type" type="java.lang.Integer"/>
		<property name="RESOURCE_ID" type="java.lang.String" length="64"/>
		<property name="RESOURCE_STATUS" type="java.lang.Integer"/>
		<property name="RESOURCE_DESCRIBE" type="java.lang.String" length="256"/>
		<property name="DATASET_SENSITIVE_LEVEL" type="java.lang.String" length="64"/>
		<property name="DATA_SET" type="java.lang.String" length="64"/>
		<property name="SECTION_CLASS" type="java.lang.String" length="64"/>
		<property name="ELEMENT" type="java.lang.String" length="64"/>
		<property name="SECTION_RELATIOIN_CLASS" type="java.lang.String" length="64"/>
		<property name="OPERATE_SYMBOL" type="java.lang.String" length="64"/>
		<property name="ELEMENT_VALUE" type="java.lang.String" length="64"/>
		<property name="DELETE_STATUS" type="java.lang.Integer"/>
		<property name="DATA_VERSION" type="java.lang.Integer"/>
		<property name="LATEST_MOD_TIME" type="java.lang.String" length="64"/>
		<property name="RESOURCE_REMARK" type="java.lang.String" length="256"/>
	</class>
</hibernate-mapping>
	
