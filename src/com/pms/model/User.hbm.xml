<?xml version="1.0" encoding="UTF-8"?>
<!-- 指定Hiberante3映射文件的DTD信息 -->
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.pms.model">
	<!-- 每个class元素对应一个持久化对象 -->
	<class name="User" table="WA_AUTHORITY_POLICE">
		<!-- id元素定义持久化类的标识属性 -->
		<id name="id" type="java.lang.Integer">
			<generator class="identity"/>
		</id>
		<!-- property元素定义常规属性 -->
<!-- 		<property name="NAME" type="java.lang.String" length="64" not-null="true"/> -->
		<property name="NAME" type="java.lang.String" length="64"/>
		<property name="CERTIFICATE_CODE_MD5" type="java.lang.String" length="32"/>
		<property name="CERTIFICATE_CODE_SUFFIX" type="java.lang.String" length="32"/>
		<property name="SEXCODE" type="java.lang.String" length="16"/>
		<property name="GA_DEPARTMENT" type="java.lang.String" length="64" />
		<property name="UNIT" type="java.lang.String" length="64" />
		<property name="ORG_LEVEL" type="java.lang.String" length="64"/>
		<property name="POLICE_SORT" type="java.lang.String" length="64"/>
		<property name="POLICE_NO" type="java.lang.String" length="64"/>
		<property name="SENSITIVE_LEVEL" type="java.lang.String" length="64"/>
		<property name="BUSINESS_TYPE" type="java.lang.String" length="64"/>
		<property name="TAKE_OFFICE" type="java.lang.String" length="64"/>
		<property name="USER_STATUS" type="java.lang.Integer"/>
		<property name="position" type="java.lang.String" length="64"/>
		<property name="dept" type="java.lang.String" length="64"/>	
		<property name="DELETE_STATUS" type="java.lang.Integer"/>
		<property name="DATA_VERSION" type="java.lang.Integer"/>
		<property name="LATEST_MOD_TIME" type="java.lang.String" length="64"/>
	</class>
</hibernate-mapping>

