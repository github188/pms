<?xml version="1.0" encoding="UTF-8"?>
<!-- 指定Hiberante3映射文件的DTD信息 -->
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.pms.model">
	<!-- 每个class元素对应一个持久化对象 -->
	<class name="User" table="user">
		<!-- id元素定义持久化类的标识属性 -->
		<id name="id" type="java.lang.Integer">
			<generator class="identity"/>
		</id>
		<!-- property元素定义常规属性 -->
		<property name="name" type="java.lang.String" length="64" not-null="true"/>
		<property name="status" type="java.lang.Integer"/>
		<property name="parent_id" type="java.lang.Integer"/>
		<property name="unit" type="java.lang.String" length="128"/>
		<property name="police_type" type="java.lang.String" length="128"/>
		<property name="sex" type="java.lang.String" length="128"/>
		<property name="idnum" type="java.lang.String" length="128"/>
		<property name="max_sensitive_level" type="java.lang.String" length="128"/>
		<property name="position" type="java.lang.String" length="128"/>
		<property name="dept" type="java.lang.String" length="128"/>
		<property name="title" type="java.lang.String" length="128"/>
		<property name="police_num" type="java.lang.String" length="128"/>
		<property name="tstamp" type="java.lang.String" length="64"/>
	</class>
</hibernate-mapping>