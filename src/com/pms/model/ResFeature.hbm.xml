<?xml version="1.0" encoding="UTF-8"?>
<!-- 指定Hiberante3映射文件的DTD信息 -->
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.pms.model">
	<!-- 每个class元素对应一个持久化对象 -->
	<class name="ResFeature" table="WA_AUTHORITY_FUNC_RESOURCE">
		<!-- id元素定义持久化类的标识属性 -->
		<id name="id" type="java.lang.Integer">
			<generator class="identity"/>
		</id>
		<!-- property元素定义常规属性 -->
		<property name="SYSTEM_TYPE" type="java.lang.String" length="64"/>
		<property name="RESOURCE_ID" type="java.lang.String" length="64"/>
		<property name="APP_ID" type="java.lang.String" length="64"/>
		<property name="RESOUCE_NAME" type="java.lang.String" length="64"/>
		<property name="PARENT_RESOURCE" type="java.lang.String" length="64"/>
		<property name="URL" type="java.lang.String" length="128"/>
		<property name="RESOURCE_ICON_PATH" type="java.lang.String" length="128"/>
		<property name="RESOURCE_STATUS" type="java.lang.Integer"/>
		<property name="RESOURCE_ORDER" type="java.lang.String" length="64"/>
		<property name="RESOURCE_DESCRIBE" type="java.lang.String" length="256"/>
		<property name="RMK" type="java.lang.String" length="256"/>
		<property name="FUN_RESOURCE_TYPE" type="java.lang.Integer"/>
		<property name="DELETE_STATUS" type="java.lang.Integer"/>
		<property name="DATA_VERSION" type="java.lang.Integer"/>
		<property name="LATEST_MOD_TIME" type="java.lang.String" length="64"/>
	</class>
</hibernate-mapping>