<?xml version="1.0" encoding="UTF-8"?>
<!-- 指定Hiberante3映射文件的DTD信息 -->
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.pms.model">
	<!-- 每个class元素对应一个持久化对象 -->
	<class name="ResRole" table="res_role">
		<!-- id元素定义持久化类的标识属性 -->
		<id name="id" type="java.lang.Integer">
			<generator class="identity"/>
		</id>
		<!-- property元素定义常规属性 -->
		<property name="business_role_name" type="java.lang.String" length="64" not-null="true"/>
		<property name="business_role" type="java.lang.String" length="64"/>
		<property name="business_role_type" type="java.lang.Integer"/>	
		<property name="role_describe" type="java.lang.String" length="256"/>
		<property name="clue_dst_sys" type="java.lang.String" length="256"/>
		<property name="system_type" type="java.lang.String" length="32"/>
		<property name="clue_src_sys" type="java.lang.String" length="256"/>
		<property name="delete_status" type="java.lang.Integer"/>
		
		<property name="latest_mod_time" type="java.lang.String" length="64"/>
	</class>
</hibernate-mapping>
